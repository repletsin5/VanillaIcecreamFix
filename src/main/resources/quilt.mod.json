{
  "schema_version": 1,
  "quilt_loader": {
    "group": "me.replet",
    "id": "vanillafix",
    "version": "${version}",
    "metadata": {
      "name": "VanillaIcecreamFix",
      "description": "Bug fixes and optimizations for Minecraft",
      "contributors": {
        "Runemoro (Original)": "Owner",
        "replet": "Owner"
      },
      "contact": {
        "homepage": "https://example.com/",
        "issues": "https://github.com/QuiltMC/quilt-template-mod/issues",
        "sources": "https://github.com/QuiltMC/quilt-template-mod"
      },
      "icon": "assets/logo.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",

    "depends": [
